{
  "name": "spider-cobweb",
  "version": "0.0.1",
  "description": "an UI library for patch",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js --coverage",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "ci": "cross-env NODE_ENV=test JEST_JUNIT_OUTPUT=./test-results/jest/results.xml jest --config=jest.config.js --coverage && cat ./reports/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint:fix": "npx eslint --fix \"{components,utils,admin}/**/*.ts{,x}\"",
    "changelog": "conventional-changelog -p angular -o CHANGELOG.md -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teobler/spider-cobweb.git"
  },
  "keywords": [],
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run lint:fix"
    }
  },
  "author": "teobler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teobler/spider-cobweb/issues"
  },
  "homepage": "https://github.com/teobler/spider-cobweb#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "acorn": "^6.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.3.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-react-app": "^7.0.2",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "jest-haste-map": "^24.0.0",
    "jest-junit": "^6.3.0",
    "jest-resolve": "^24.1.0",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "webpack-dev-server": "^3.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
